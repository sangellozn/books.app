package info.san.books.app.exception;

/**
 * If the ISBN if already known in the system.
 *
 * @author ANGELLOZ-NICOUD SÃ©bastien
 *
 */
public class ISBNAlreadyTakenException extends NameAlreadyTakenException {

	private static final long serialVersionUID = -5147814652185220227L;

	public ISBNAlreadyTakenException() {
		super();
	}

	public ISBNAlreadyTakenException(String message) {
		super(message);
	}

	public ISBNAlreadyTakenException(Throwable cause) {
		super(cause);
	}

	public ISBNAlreadyTakenException(String message, Throwable cause) {
		super(message, cause);
	}

	public ISBNAlreadyTakenException(String message, Throwable cause,
			boolean enableSuppression, boolean writableStackTrace) {
		super(message, cause, enableSuppression, writableStackTrace);
	}

}
