package info.san.biblio.app.event.serie;

public class SerieUpdatedEvent {

	private String id;

	private String name;

	public SerieUpdatedEvent(String id, String name) {
		this.id   = id;
		this.name = name;
	}

	public String getId() {
		return this.id;
	}

	public String getName() {
		return this.name;
	}

}
