package info.san.biblio.app.command.serie;

import java.util.UUID;

import org.axonframework.commandhandling.annotation.TargetAggregateIdentifier;

public class SerieCreateCommand {
	
	@TargetAggregateIdentifier
	private String id;
	
	private String name;
	
	public SerieCreateCommand(String name) {
		this.id   = UUID.randomUUID().toString();
		this.name = name;
	}

	public String getId() {
		return id;
	}
	
	public String getName() {
		return name;
	}
	
}
