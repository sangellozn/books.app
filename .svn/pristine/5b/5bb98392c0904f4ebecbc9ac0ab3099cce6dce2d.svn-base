package info.san.biblio.app.event.serie;

public class SerieCreatedEvent {
	
	private String id;
	
	private String name;
	
	public SerieCreatedEvent(String id, String name) {
		this.id   = id;
		this.name = name;
	}

	public String getId() {
		return id;
	}

	public String getName() {
		return name;
	}
	
}
