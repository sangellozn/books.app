package info.san.books.app.event.livre;

import java.io.Serializable;

public class LivreDeletedEvent implements Serializable {

	private static final long serialVersionUID = 9142477185382262875L;

	private String isbn;

	public LivreDeletedEvent(String isbn) {
		this.isbn = isbn;
	}

	public String getIsbn() {
		return this.isbn;
	}

}
